{
  "expo": {
    "name": "minuet",
    "scheme": "minuet",
    "slug": "minuet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "white"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#2B2B2E"
      },
      "package": "com.minuet.www",
      "softwareKeyboardLayoutMode": "pan",
      "useNextNotificationsApi" : true,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/Logo_white.png"
    },
    "extra": {
      "eas": {
        "projectId": "d728723b-f2f5-4c3b-81fe-acbcfe952322"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b3a00ac2-ab40-405a-a409-14c1318f1609"
    },
    "plugins": [
      "@react-native-firebase/app"
    ]
  }
}
